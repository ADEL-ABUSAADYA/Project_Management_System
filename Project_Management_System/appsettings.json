{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IpRateLimitOptions": {
    "EnableEndpointRateLimiting": "True",
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 5
      }
    ],
    "EndpointWhiteList": [
      "Get:/LogInUserEndpoint/LogInUser"
    ]
  },
  "JWTSettings": {
    "Issuer": "UpSkilling",
    "Audience": "UpSkilling-Audience"
  },
  "OTPSettings": {
    "Issuer": "OTP",
    "Audience": "USERS"
  },
  "AllowedHosts": "*"
}
